[
  {
    "name": "delete-older-than-30-days",
    "action": { "type": "Delete" },
    "condition": {
      "olderThan": "30d"
    }
  },
  {
    "name": "keep-latest-release",
    "action": { "type": "Keep" },
    "condition": {
      "tagState": "tagged",
      "tagPrefixes": ["latest"]
    }
  }
]
